<!-- Page Content -->
<div id="page-content">
  <!-- Breadcrumb -->
  <div class="container">
    <ol class="breadcrumb">
      <li><%= link_to "Home", root_path %></li>
      <li class="active">Latest Import</li>
    </ol>
    </div>
    <!-- end Breadcrumb -->
    <div class="container">
      <header><h1>Latest Import</h1></header>
      <div class="row">
        <div class="col-md-4 col-sm-6 col-md-offset-4 col-sm-offset-3">
          <section id="quick-summary" class="clearfix">
              <header><h2 id="latestImportHeader">Fetching Latest Import:</h2></header>
              <dl id="import-info">
                <dt>Last import started at:</dt>
                <dd><%= @import.time_started.strftime("%m/%d/%y %l:%M %p") %> MT</dd>
                <dt>Previous number of active listings:</dt>
                <dd><%= @import.prev_quantity_active %></dd>
                <div id="loadingIcon" class="sk-fading-circle">
                  <div class="sk-circle1 sk-circle"></div>
                  <div class="sk-circle2 sk-circle"></div>
                  <div class="sk-circle3 sk-circle"></div>
                  <div class="sk-circle4 sk-circle"></div>
                  <div class="sk-circle5 sk-circle"></div>
                  <div class="sk-circle6 sk-circle"></div>
                  <div class="sk-circle7 sk-circle"></div>
                  <div class="sk-circle8 sk-circle"></div>
                  <div class="sk-circle9 sk-circle"></div>
                  <div class="sk-circle10 sk-circle"></div>
                  <div class="sk-circle11 sk-circle"></div>
                  <div class="sk-circle12 sk-circle"></div>
                </div>
                <div id="newImportInfo">
                  <dt>Completed in:</dt>
                  <dd id="ImportCompletedIn"></dd>
                  <dt>Current number of active listings:</dt>
                  <dd id="ImportCurrentActiveRecords"></dd>
                </div>
              </dl>
            </section><!-- /#quick-summary -->
          <hr>
       </div>
     </div>
   </div>
 </div>
